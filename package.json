{
  "name": "minecraft-validator",
  "version": "1.0.0",
  "description": "A simple wrapper for Mojang's Minecraft Creator Tools CLI. Allowing for more robust error handling to prevent false-postives when integrating into CI/CD workflows.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/index.js"
  },
  "keywords": ["minecraft", "validator", "bedrock"],
  "author": "Caleb Coldiron",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/papaparse": "^5.3.16",
    "@types/picomatch": "^4.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@minecraft/creator-tools": "^0.11.0",
    "chalk": "^5.4.1",
    "papaparse": "^5.5.3",
    "picomatch": "^4.0.2"
  }
}
